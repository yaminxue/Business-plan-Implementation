<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.SSH.Beans">  
	<class name="GPS" table="gps_info">
		<id name="user_ID" column="user_ID">
			<generator class="assigned"/> 
		</id>
		 <property name="longtitude" column="longtitude" type="java.lang.Double"  
            not-null="false" ></property>  
        <property name="latitude" column="latitude" type="java.lang.Double"
        	not-null="false" ></property>
        <property name="datetime" column="update_date" type="java.sql.Timestamp"
        	not-null="false" ></property>
	</class>

</hibernate-mapping>